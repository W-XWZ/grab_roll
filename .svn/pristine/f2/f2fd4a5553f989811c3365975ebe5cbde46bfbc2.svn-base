body {
            font-family: 'Microsoft Yahei';
            width: 600px;
            height: 306px;
        }

        a {
            margin-right: 10px;
        }

        .click-button{
            text-decoration:none;

            font-family: 微软雅黑,宋体,Arial,Helvetica,Verdana,sans-serif;
            font-weight:bold;
            border-radius:3px;

            -webkit-transition:all linear 0.30s;
            -moz-transition:all linear 0.30s;
            ont-size: 14.4px;
            color: #FFFFFF;
            letter-spacing: 0.34px;
            text-align: center;
            background-color: #629DFC;
            float: right;
            margin-right: 10px;
            width: 96px;
            height: 32px;
        }

        .prior-button{
            margin-top: 5px;
            margin-bottom: 5px;
            font-size: 14.4px;
            color: #FFFFFF;
            letter-spacing: 0.34px;
            text-align: center;
            background-color: #629DFC;
        }

        .add-condition-btn{
            font-size: 20.8px;
            color: #629DFC;
            letter-spacing: 0.49px;
            text-align: center;
            width: 86px;
            height: 29px;
        }

        .ware-amount{
            color: #333333;
            font-size: 14px;
            margin-top: 10px;
        }

        .condition-input{
            font-size: 14px;
            letter-spacing: 0.33px;
            text-align: center;
        }